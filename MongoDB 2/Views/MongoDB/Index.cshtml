@model IEnumerable<MongoDB_2.Models.MongoDBModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Oyuncu)
            @Html.DisplayNameFor(model => model.Mevki)
            @Html.DisplayNameFor(model => model.Bonservis)
            @Html.DisplayNameFor(model => model.Kulup)
            @Html.DisplayNameFor(model => model.Numara)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Oyuncu)
            @Html.DisplayFor(modelItem => item.Mevki)
            @Html.DisplayFor(modelItem => item.Bonservis)
            @Html.DisplayFor(modelItem => item.Kulup)
            @Html.DisplayFor(modelItem => item.Numara)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
